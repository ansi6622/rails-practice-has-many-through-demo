<div class="page-header">
  <h1>Tags</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>
        Name
      </th>
    </tr>
  </thead>
  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td>
          <%= link_to tag.name, tag_path(tag) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
